{"ast":null,"code":"import { ref } from 'vue';\nimport ProductList from './components/ProductList.vue';\nimport AddProductForm from './components/AddProductForm.vue';\n// eslint-disable-next-line no-unused-vars\nimport store from './store/store.js'; // Import your Vuex store\n\nexport default {\n  name: 'App',\n  components: {\n    ProductList,\n    AddProductForm\n  },\n  setup() {\n    const products = ref([\n    // Initial dummy products, you can fetch actual products from Vuex store\n    {\n      id: 1,\n      name: 'Noli Me Tangere',\n      description: 'lorem ipsum',\n      price: 250\n    }, {\n      id: 2,\n      name: 'El Filibusterismo',\n      description: 'lorem ipsum',\n      price: 200\n    }, {\n      id: 3,\n      name: 'Ibong Adarna',\n      description: 'lorem ipsum',\n      price: 350\n    }]);\n    const showAdded = ref(false);\n    const addProduct = () => {\n      // Perform addition of product to the list\n      // For demonstration, let's just show the animation for now\n      showAdded.value = true;\n      setTimeout(() => {\n        showAdded.value = false;\n      }, 1500); // Adjust the duration of the animation\n    };\n    return {\n      products,\n      addProduct,\n      showAdded\n    };\n  }\n};","map":{"version":3,"names":["ref","ProductList","AddProductForm","store","name","components","setup","products","id","description","price","showAdded","addProduct","value","setTimeout"],"sources":["C:\\midtermact2\\src\\App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    \n\n    <transition name=\"fade\">\n      <div v-if=\"showAdded\" class=\"added-product\">\n        Product Added!\n      </div>\n    </transition>\n\n    <ProductList :products=\"products\" />\n    <AddProductForm />\n  </div>\n</template>\n\n<script>\nimport { ref } from 'vue';\nimport ProductList from './components/ProductList.vue';\nimport AddProductForm from './components/AddProductForm.vue';\n// eslint-disable-next-line no-unused-vars\nimport store from './store/store.js'; // Import your Vuex store\n\nexport default {\n  name: 'App',\n  components: {\n    ProductList,\n    AddProductForm\n  },\n  setup() {\n    const products = ref([\n      // Initial dummy products, you can fetch actual products from Vuex store\n      { id: 1, name: 'Noli Me Tangere', description: 'lorem ipsum', price: 250 },\n      { id: 2, name: 'El Filibusterismo', description: 'lorem ipsum', price: 200 },\n      { id: 3, name: 'Ibong Adarna', description: 'lorem ipsum', price: 350 },\n    ]);\n\n    const showAdded = ref(false);\n\n    const addProduct = () => {\n      // Perform addition of product to the list\n      // For demonstration, let's just show the animation for now\n      showAdded.value = true;\n      setTimeout(() => {\n        showAdded.value = false;\n      }, 1500); // Adjust the duration of the animation\n    };\n\n    return {\n      products,\n      addProduct,\n      showAdded\n    };\n  }\n}\n</script>\n\n<style scoped>\n.add-product-btn {\n  margin-bottom: 20px;\n}\n\n.added-product {\n  background-color: lightgreen;\n  padding: 10px;\n  margin-bottom: 10px;\n}\n\n.fade-enter-active, .fade-leave-active {\n  transition: opacity 0.5s ease-in-out;\n}\n\n.fade-enter, .fade-leave-to {\n  opacity: 0;\n}\n</style>\n"],"mappings":"AAgBA,SAASA,GAAE,QAAS,KAAK;AACzB,OAAOC,WAAU,MAAO,8BAA8B;AACtD,OAAOC,cAAa,MAAO,iCAAiC;AAC5D;AACA,OAAOC,KAAI,MAAO,kBAAkB,EAAE;;AAEtC,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE;IACVJ,WAAW;IACXC;EACF,CAAC;EACDI,KAAKA,CAAA,EAAG;IACN,MAAMC,QAAO,GAAIP,GAAG,CAAC;IACnB;IACA;MAAEQ,EAAE,EAAE,CAAC;MAAEJ,IAAI,EAAE,iBAAiB;MAAEK,WAAW,EAAE,aAAa;MAAEC,KAAK,EAAE;IAAI,CAAC,EAC1E;MAAEF,EAAE,EAAE,CAAC;MAAEJ,IAAI,EAAE,mBAAmB;MAAEK,WAAW,EAAE,aAAa;MAAEC,KAAK,EAAE;IAAI,CAAC,EAC5E;MAAEF,EAAE,EAAE,CAAC;MAAEJ,IAAI,EAAE,cAAc;MAAEK,WAAW,EAAE,aAAa;MAAEC,KAAK,EAAE;IAAI,CAAC,CACxE,CAAC;IAEF,MAAMC,SAAQ,GAAIX,GAAG,CAAC,KAAK,CAAC;IAE5B,MAAMY,UAAS,GAAIA,CAAA,KAAM;MACvB;MACA;MACAD,SAAS,CAACE,KAAI,GAAI,IAAI;MACtBC,UAAU,CAAC,MAAM;QACfH,SAAS,CA